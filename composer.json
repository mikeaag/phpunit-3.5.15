{
    "name": "mikeaag/phpunit-3.5.15",
    "description": "PHPUnit v3.5.15 which is compatible with ZendFramework 1.12.20",
    "type": "library",
    "keywords": [
        "phpunit",
        "xunit",
        "testing"
    ],
    "homepage": "https://phpunit.de/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sebastian Bergmann",
            "email": "sebastian@phpunit.de",
            "role": "lead"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpunit/php-file-iterator": "~1.3",
        "phpunit/php-text-template": "~1.1",
        "phpunit/php-code-coverage": "~1.2",
        "phpunit/php-timer": "~1.0",
        "phpunit/phpunit-mock-objects": "~1.2",
        "symfony/yaml": "~2.0",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-pcre": "*",
        "ext-reflection": "*",
        "ext-spl": "*"
    },
    "require-dev": {
    },
    "bin": [
        "composer/bin/phpunit"
    ],
    "autoload": {
        "classmap": [
            "PHPUnit/"
        ]
    }
